    },
    "CERTIFICATE_BUNDLE_MD5SUM": {
      "$id": "#/properties/CERTIFICATE_BUNDLE_MD5SUM",
      "type": "string",
      "title": "The CERTIFICATE_BUNDLE_MD5SUM schema",
