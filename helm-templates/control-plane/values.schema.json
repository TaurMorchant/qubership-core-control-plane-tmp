    "SSL_SECRET": {
      "$id": "#/properties/SSL_SECRET",
      "type": "string",
      "title": "The SSL_SECRET schema",
      "description": "SSL secret name.",
      "examples": [
        "defaultsslcertificate"
      ],
      "internal": true
    },
    "TAG": {
      "$id": "#/properties/TAG",
      "type": "string",
      "title": "The TAG schema",
      "internal": true
