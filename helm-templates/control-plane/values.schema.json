      "description": "Specify .spec.strategy.rollingUpdate.maxSurge parameter",
      "internal": false,
      "envSpecific": false,
      "examples": [
        "50%",
        "2"
      ]
    },
    "DEPLOYMENT_STRATEGY_MAXUNAVAILABLE": {
      "$id": "#/properties/DEPLOYMENT_STRATEGY_MAXUNAVAILABLE",
      "type": "string",
